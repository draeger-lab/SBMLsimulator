<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
  <comment>This file captures the default configuration of SBMLsimulator</comment>
  
  <!-- Command-line argument values -->
  <entry key="SBML_FILE"></entry>
  <entry key="TIME_SERIES_FILE"></entry>
  
  <!-- General -->
  <entry key="OPEN_DIR">user.home</entry>
  <entry key="SAVE_DIR">user.home</entry>
  <entry key="SPINNER_MIN_VALUE">-1E5d</entry>
  <entry key="SPINNER_MAX_VALUE">1E5d</entry>
  <entry key="SPINNER_STEP_SIZE">0.01</entry>
  
  <!-- Parsing -->
  <entry key="CSV_FILES_OPEN_DIR">user.home</entry>
  <entry key="CSV_FILES_SAVE_DIR">user.home</entry>
  <entry key="CSV_FILES_QUOTE_CHAR">'</entry>
  <entry key="CSV_FILES_SEPARATOR_CHAR">,</entry>
  
  <!-- Plot settings -->
  <entry key="PLOT_SAVE_DIR">user.home</entry>
  <entry key="PLOT_LOG_SCALE">false</entry>
  <entry key="PLOT_SHOW_GRID">true</entry>
  <entry key="PLOT_SHOW_LEGEND">true</entry>
  <entry key="PLOT_SHOW_TOOLTIPS">false</entry>
  <entry key="JPEG_COMPRESSION_FACTOR">0.8f</entry>
  
  <!-- Simulation settings -->
  <entry key="SIM_MAX_COMPARTMENT_SIZE">100000.0</entry>
  <entry key="SIM_MAX_SPECIES_VALUE">100000.0</entry>
  <entry key="SIM_MAX_PARAMETER_VALUE">100000.0</entry>
 
  <!-- Computing -->
  <entry key="SIM_ODE_SOLVER">org.sbml.simulator.math.odes.RKEventSolver</entry>
  <entry key="SIM_DISTANCE_FUNCTION">org.sbml.simulator.math.RSE</entry>
  <entry key="SIM_DISTANCE_DEFAULT_VALUE">10000d</entry>
  <entry key="SIM_DISTANCE_ROOT">2d</entry>
  <entry key="SIM_MAX_TIME">100000.0</entry>
  <entry key="SIM_START_TIME">0.0</entry>
  <entry key="SIM_END_TIME">5.0</entry>
  <entry key="SIM_MAX_STEPS_PER_UNIT_TIME">500</entry>
  <entry key="SIM_STEP_SIZE">0.01</entry>
  
  <!-- Default values -->
  <entry key="OPT_DEFAULT_COMPARTMENT_INITIAL_SIZE">1.0d</entry>
  <entry key="OPT_DEFAULT_SPECIES_INITIAL_VALUE">1.0d</entry>
  <entry key="OPT_DEFAULT_VALUE_OF_NEW_PARAMETERS">1.0d</entry>
  
  <!-- Optimization, i.e., estimation -->
  <entry key="EST_INIT_MIN_VALUE">0d</entry>
  <entry key="EST_INIT_MAX_VALUE">14000d</entry>
  <entry key="EST_MIN_VALUE">0</entry>
  <entry key="EST_MAX_VALUE">14000d</entry>
  <entry key="EST_ALL_LOCAL_PARAMETERS">true</entry>
  <entry key="EST_ALL_GLOBAL_PARAMETERS">false</entry>
  <entry key="EST_ALL_COMPARTMENTS">false</entry>
  <entry key="EST_ALL_SPECIES">false</entry>
  <entry key="EST_MULTI_SHOOT">true</entry>
  
</properties>
