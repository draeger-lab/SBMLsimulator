<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
  <comment>
	   This configuration describes the names of buttons and menu
	   items in base graphical user interfaces. To change the configuration
	   note that the key is the same as the enum type in BaseFrame.BaseAction
	   and the given value consists of the actual short name and - separated
	   by a semicolon ";" the corresponding tool tip.
  </comment>
  
  <!-- DynamicControlPanel -->
  
  <entry key="PLAY">Play</entry>
  <entry key="PLAY_TOOLTIP">Start the dynamic visualization.</entry>
  <entry key="PAUSE">Pause</entry>
  <entry key="PAUSE_TOOLTIP">Pause the dynamic visualization.</entry>
  <entry key="STOP">Stop</entry>
  <entry key="STOP_TOOLTIP">Stop the dynamic visualization.</entry>
  <entry key="TOVIDEO">Video</entry>
  <entry key="TOVIDEO_TOOLTIP">Save the dynamic visualization as a video.</entry>
  <entry key="GRAPHSHOT">Image</entry>
  <entry key="GRAPHSHOT_TOOLTIP">Take a screenshot of the whole graph.</entry>  
  <entry key="NODELABELS">Nodelabels</entry>
  <entry key="NODELABELS_TOOLTIP">Switch for node labels.</entry>
  <entry key="REACTIONLABELS">Reactionlabels</entry>
  <entry key="REACTIONLABELS_TOOLTIP">Switch for reaction labels.</entry>
  <entry key="FAST">Fast</entry>
  <entry key="NORMAL">Normal</entry>
  <entry key="SLOW">Slow</entry>
  <entry key="FALSE_INPUT">Input not a positive integer.</entry>
  <entry key="TIMEPOINT">Timepoint</entry>
  <entry key="SIMULATIONSPEED">Simulationspeed</entry>
  <entry key="SIMULATIONSPEED_SPINNER_TOOLTIP">Displays the current simulation speed as value. Changeable in Options/Network.</entry>
  <entry key="NODESIZE">Node size</entry>
  <entry key="NODECOLOR">Node color</entry>
  <entry key="GROUP_VISUALIZATION">Visualization</entry> 
  <entry key="SIMULATION_DATA">Simulation Data</entry> 
  <entry key="EXPERIMENTAL_DATA">Experimental Data</entry>   
  <entry key="SEARCHBAR_TOOLTIP">Choose time point of visualization.</entry>
  <entry key="VIDEO_CODEC_NOT_FOUND">Couldn't guess video codec. Stated file ending/format correct?</entry>
  <entry key="COULD_NOT_WRITE_SCREENSHOT">Couldn't write screenshot file.</entry>
  <entry key="FILEFILTER_SCREENSHOT">Portable Network Graphics (.png)</entry>
  
  <!-- GraphOptions -->

  <entry key="GraphOptions">Network</entry>
  <entry key="MIN_NODE_SIZE">Minimum node size</entry>
  <entry key="MIN_NODE_SIZE_TOOLTIP">In case of dynamic change of node size, this determines the minimum node size.</entry>
  <entry key="MAX_NODE_SIZE">Maximum node size</entry>
  <entry key="MAX_NODE_SIZE_TOOLTIP">In case of dynamic change of node size, this determines the maximum node size.</entry>
  <entry key="USE_UNIFORM_NODE_COLOR">Uniform node color</entry>
  <entry key="USE_UNIFORM_NODE_COLOR_TOOLTIP">Enable/Disable uniform node colors instead of node colors given in legend.</entry>
  <entry key="UNIFORM_NODE_COLOR">Color for uniform node color</entry>
  <entry key="UNIFORM_NODE_COLOR_TOOLTIP">Sets the color for uniform node color.</entry>
  <entry key="GROUP_NODESIZE">Dynamic change of node size</entry>
  <entry key="GROUP_NODESIZE_TOOLTIP">Options for the dynamic change of node size.</entry>
  <entry key="GROUP_NODECOLOR">Dynamic change of node color</entry>
  <entry key="GROUP_NODECOLOR_TOOLTIP">Options for the dynamic change of node color.</entry>
  <entry key="GROUP_REACTION">Dynamical change of reactions</entry>
  <entry key="GROUP_REACTION_TOOLTIP">Options for the dynamic change of reactions.</entry>
  <entry key="COLOR1">High concentration</entry>
  <entry key="COLOR1_TOOLTIP">In case of dynamic change of node color, this determines the color for high concentration.</entry>
  <entry key="COLOR2">Middle concentration</entry>
  <entry key="COLOR2_TOOLTIP">In case of dynamic change of node color, this determines the color for middle concentration.</entry>
  <entry key="COLOR3">Low concentration</entry>
  <entry key="COLOR3_TOOLTIP">In case of dynamic change of node color, this determines the color for low concentration.</entry>  
  <entry key="MIN_LINE_WIDTH">Minimum line width</entry>
  <entry key="MIN_LINE_WIDTH_TOOLTIP">Determines the minimum line width of dynamic reaction visualization.</entry> 
  <entry key="MAX_LINE_WIDTH">Maximum line width</entry>
  <entry key="MAX_LINE_WIDTH_TOOLTIP">Determines the maximum line width of dynamic reaction visualization.</entry>
  <entry key="SHOW_NODE_LABELS">Node labels</entry>
  <entry key="SHOW_NODE_LABELS_TOOLTIP">Enables/Disables node labels.</entry>
  <entry key="SHOW_REACTION_LABELS">Reaction labels</entry>
  <entry key="SHOW_REACTION_LABELS_TOOLTIP">Enables/Disables reaction labels.</entry>
  <entry key="GRAPH_LABELS">Labeling</entry>
  <entry key="GRAPH_LABELS_TOOLTIP">Enables/Disables labels.</entry>
  <entry key="COLOR_NODE_SIZE">Constant node size</entry>
  <entry key="COLOR_NODE_SIZE_TOOLTIP">Determines the constant node size in case of dynamic change of node color.</entry>
  <entry key="SIM_SPEED_FAST">Fast play speed</entry>
  <entry key="SIM_SPEED_FAST_TOOLTIP">Sets setting for fast play speed.</entry>
  <entry key="SIM_SPEED_NORMAL">Normal play speed</entry>
  <entry key="SIM_SPEED_NORMAL_TOOLTIP">Sets setting for normal play speed.</entry>  
  <entry key="SIM_SPEED_SLOW">Slow play speed</entry>
  <entry key="SIM_SPEED_SLOW_TOOLTIP">Sets setting for slow play speed.</entry>
  <entry key="SIM_SPEED_CHOOSER">Choose play speed</entry>
  <entry key="SIM_SPEED_CHOOSER_TOOLTIP">This options determines the play speed.</entry>  
  <entry key="GROUP_SIM_SPEED">Play speed</entry>
  <entry key="GROUP_SIM_SPEED_TOOLTIP">These settings control the play speed. The smaller the value, the faster the play speed.</entry>
  <entry key="VISUALIZATION_STYLE">Visualization style</entry>
  <entry key="VISUALIZATION_STYLE_TOOLTIP">This options determines the visualization style.</entry>  
  <entry key="VISUALIZATION_DATA">Data for visualization</entry>
  <entry key="VISUALIZATION_DATA_TOOLTIP">Choose the data to visualize.</entry>  
  <entry key="DATA_NOT_AVAILABLE">Chosen data set for visualization is not available by now.</entry>
  <entry key="GROUP_VIDEO_ENCODING">Video encoding</entry>
  <entry key="GROUP_VIDEO_ENCODING_TOOLTIP">These settings control video encoding.</entry>
  <entry key="VIDEO_FRAMERATE">Framerate</entry>
  <entry key="VIDEO_FRAMERATE_TOOLTIP">The lower the framerate, the slower the video play speed.</entry>
  <entry key="VIDEO_IMAGE_STEPSIZE">Steps between two images</entry>
  <entry key="VIDEO_IMAGE_STEPSIZE_TOOLTIP">The less steps are chosen, the less images are taken to encode the video.</entry>    
  <entry key="VIDEO_RESOLUTION_MULTIPLIER">Resolution multiplier</entry>
  <entry key="VIDEO_RESOLUTION_MULTIPLIER_TOOLTIP">The higher this multiplier, the better the resolution but the higher the outcoming file size.</entry>
  
</properties>
